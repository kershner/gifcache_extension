{
	"manifest_version": 2,
	"name": "GifCache Chrome Extension",
	"description": "Save GIFs to your Cache from anywhere on the internet",
	"version": "1.0",
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "popup.html",
		"default_title": "GifCache"
	},
	"permissions": [
		"activeTab",
		"cookies",
		"contextMenus",
		"storage",
		"https://ajax.googleapis.com/",
		"http://www.gifcache.com/"
	],
	"icons": {
		"16": "img/icon.png"
	},
	"background": {
		"scripts": ["js/eventPage.js"],
		"persistent": true
	}
}